{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["../README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true,
		"default": {
			"layoutFile" : "doclayout.tmpl"
		}
    },
    "opts": {
        "destination": "../docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true
    }
}